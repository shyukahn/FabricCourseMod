{
  "schemaVersion": 1,
  "id": "mccourse",
  "version": "${version}",
  "name": "MCCourse Mod",
  "description": "MCCourse Mod for Minecraft Fabric Modding",
  "authors": [
    "Sehyuk Ahn"
  ],
  "contact": {
    "issues": "https://github.com/shyukahn/FabricCourseMod/issues",
    "sources": "https://github.com/shyukahn/FabricCourseMod"
  },
  "icon": "assets/mccourse/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "com.shyukahn.mccourse.MCCourse"
      }
    ],
    "client": [
      {
        "adapter": "kotlin",
        "value": "com.shyukahn.mccourse.MCCourseClient"
      }
    ]
  },
  "mixins": [
    "mccourse.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.13.3",
    "fabric": "*",
    "fabric-language-kotlin": ">=1.7.1+kotlin.1.6.10",
    "minecraft": "1.18.x",
    "java": ">=17"
  }
}
